<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html>

<head>

	<title>
		<%=name%>
	</title>
	<meta http-equiv="content-type" content="text/html; charset=utf-8" />

	<meta name="keywords" content="" />
	<meta name="description" content="" />

	<link rel="stylesheet" type="text/css"
		href="http://yui.yahooapis.com/2.7.0/build/reset-fonts-grids/reset-fonts-grids.css" media="all" />


</head>

<body>
	

	<style>

		.btn{
			display: inline-block;
			outline: none;
			cursor: pointer;
			font-weight: 500;
			border: 1px solid transparent;
			border-radius: 2px;
			height: 36px;
			line-height: 34px;
			font-size: 14px;
			color: #ffffff;
			background-color: #007c89;
			transition: background-color 0.2s ease-in-out 0s, opacity 0.2s ease-in-out 0s;
			padding: 0 18px;
		}
		.btn:hover {
                        color: #ffffff;
                        background-color: #006570;
                    }

		#downloadLink{
			color: white;
			text-decoration: none;
			font-size: large;
		}
		
		.msg {
			/* padding: 10px; */
			background: #222;
			position: relative;
		}

		.msg h1 {
			color: #fff;
		}

		.msg a {
			margin-left: 20px;
			background: #408814;
			color: white;
			padding: 4px 8px;
			text-decoration: none;
		}

		.msg a:hover {
			background: #266400;
		}

		.header-info{
			font-size: 18px;
		}
		/* //-- yui-grids style overrides -- */
		body {
			font-family: Garamond;
			color: #444;
		}

		#inner {
			padding: 10px 80px;
			margin: 0px auto;
			background: #f5f5f5;
		}

		.yui-gf {
			margin-bottom: 2em;
			padding-bottom: 2em;
			border-bottom: 1px solid #ccc;
		}

		/* //-- header, body, footer -- */
		#hd {
			margin: 2.5em 0 3em 0;
			padding-bottom: 1.5em;
			border-bottom: 1px solid #ccc
		}

		#hd h2 {
			text-transform: uppercase;
			letter-spacing: 2px;
		}

		#bd,
		#ft {
			margin-bottom: 2em;
		}

		/* //-- footer -- */
		#ft {
			padding: 1em 0 5em 0;
			font-size: 92%;
			border-top: 1px solid #ccc;
			text-align: center;
		}

		#ft p {
			margin-bottom: 0;
			text-align: center;
		}

		/* //-- core typography and style -- */
		#hd h1 {
			font-size: 43px;
			text-transform: uppercase;
			letter-spacing: 3px;
		}

		h2 {
			font-size: 152%
		}

		h3,
		h4 {
			font-size: 122%;
		}

		h1,
		h2,
		h3,
		h4 {
			color: #333;
		}

		p {
			font-size: 100%;
			line-height: 18px;
			padding-right: 3em;
		}

		a {
			color: #990003
		}

		a:hover {
			text-decoration: none;
		}

		strong {
			font-weight: bold;
		}

		.skills-li{
			border-bottom: none;
			list-style: decimal;
			font-size: 18px;
		}

		li {
			line-height: 24px;
			border-bottom: 1px solid #ccc;
		}

		p.enlarge {
			font-size: 18px;
			padding-right: 6.5em;
			line-height: 24px;
		}

		p.enlarge span {
			color: #000
		}

		.contact-info {
			margin-top: 7px;
		}

		.first h2 {
			font-style: italic;
		}

		.last {
			border-bottom: 0
		}


		/* //-- section styles -- */

		a#pdf {
			display: block;
			float: left;
			background: #666;
			color: white;
			padding: 6px 50px 6px 12px;
			margin-bottom: 6px;
			text-decoration: none;
		}

		a#pdf:hover {
			background: #222;
		}

		.job {
			position: relative;
			margin-bottom: 1em;
			padding-bottom: 1em;
			border-bottom: 1px solid #ccc;
		}

		.job2 {
			position: relative;
			margin-bottom: 1em;
			/* padding-bottom: 1em; */
			/* border-bottom: 1px solid #ccc; */
		}

		.job3 {
			position: relative;
			margin-bottom: 1em;
			/* padding-bottom: 1em; */
			/* border-bottom: 1px solid #ccc; */
		}

		.job h1, h2, h3, h4{
			margin-bottom: 0.6rem;
		}

		.job-desc{
			font-size: 17px;
		}

		.job h4 {
			position: absolute;
			top: 0.35em;
			right: 0
		}

		.job p {
			margin: 0.75em 0 3em 0;
		}

		.last {
			border: none;
		}

		/* .skills-list ul {
			margin: 0;
		}

		.skills-list li {
			margin: 3px 0;
			padding: 3px 0;
		}

		.skills-list li span {
			font-size: 152%;
			display: block;
			margin-bottom: -2px;
			padding: 0
		} */

		.talent {
			font-size: 16px;
			width: 32%;
			float: left
		}

		.talent h2 {
			margin-bottom: 6px;
		}

		#srt-ttab {
			margin-bottom: 100px;
			text-align: center;
		}

		#srt-ttab img.last {
			margin-top: 20px
		}

		/* --// override to force 1/8th width grids -- */
		.yui-gf .yui-u {
			width: 81.2%;
		}

		.yui-gf ul {
			width: 80%;
		}

		.yui-gf div.first {
			width: 12.3%;
		}
	</style>

	<div id="doc2" class="yui-t7">
		<div id="inner">
			<div id="hd">
				<div class="yui-gc">
					<div class="yui-u first">
						<h1>
							<%=name%>
						</h1>
						<div>
							<span class="header-info"><%=age%></span>
							<span class="header-info"><%=gender%></span>
						</div>
					</div>

					<div class="yui-u">
						<div class="contact-info">
							<h3><a href="">
									<%=email%>
								</a></h3>
							<h3>
								<%=contactNo%>
							</h3>
						</div>
						<!--// .contact-info -->
					</div>
				</div>
				<!--// .yui-gc -->
			</div>
			<!--// hd -->

			<div id="bd">
				<div id="yui-main">
					<div class="yui-b">

						<div class="yui-gf">
							<div class="yui-u first">
								<h2>Professional Summary</h2>
							</div>
							<div class="yui-u">
								<p class="enlarge">
									<%=aboutYou%>
								</p>
							</div>
						</div>

						<div class="yui-gf" style="display: flex; justify-content: space-between;">
							<div class="yui-u first" style="margin-right: 5rem;">
								<h2>Education</h2>
							</div>
							<div class="yui-u">
								<ul class="talent ">
									<li>Matriculation</li>
									<li>
										<%=schoolNameTenth%>
									</li>
									<li>
										<%=boardTenth%>
									</li>
									<li class="last">
										<%=marksInTenth%>%
									</li>
								</ul>
							</div>
							<div class="yui-u">
								<ul class="talent">
									<li>Intermediate</li>
									<li>
										<%=schoolNameTwelfth%>
									</li>
									<li>
										<%=boardTwelfth%>
									</li>
									<li class="last">
										<%=marksInTwelfth%>%
									</li>
								</ul>
							</div>
							<br>
							<div class="yui-u">
								<ul class="talent">
									<li>
										<%=universityName1%>
									</li>
									<li>
										<%=degreeName1%>
									</li>
									<li>
										<%=cgpa1%>
									</li>
									<li class='last'>
										<%= collegeName1%>
									</li>
								</ul>
							</div>
							<% if(universityName2) { %>
							<div class="yui-u">
								<ul class="talent">
									<li>
										<%=universityName2%>
									</li>
									<li>
										<%=degreeName2%>
									</li>
									<li>
										<%= cgpa2%>
									</li>
									<li  class='last'>
										<%=collegeName2%>
									</li>
								</ul>
							</div>
							<% } %>
						</div>


							<h2 style="margin-bottom: 20px">Work Experience</h2>
							<div class="job">
								<h2>
									<%= job1 %>
								</h2>
								<h3>
									<%= post1 %>
								</h3>
								<h4>
									<%= yearsWorked1 %>
								</h4>
								<p class="job-desc">
									<%= jobDesc1 %>
								</p>
								<%if(job2){ %>
								<div class="job2">
									<h2>
										<%= job2 %>
									</h2>
									<h3>
										<%= post2 %>
									</h3>
									<h4>
										<%= yearsWorked2 %>
									</h4>
									<p class="job-desc">
										<%= jobDesc2 %>
									</p>
								</div>
								<% } %>

								<%if(job3){ %>
								<div class="job3">
									<h2>
										<%= job3 %>
									</h2>
									<h3>
										<%= post3 %>
									</h3>
									<h4>
										<%= yearsWorked3 %> 
									</h4>
									<p class="job-desc">
										<%= jobDesc3 %>
									</p>
								</div>
								<% } %>
							</div>

				<!--	</div>

				 </div>  -->

<!-- 
				<div class="yui-gf last">
					<div class="yui-u first">
						<h2>References</h2>
					</div>
					<div class="yui-u">
						<h2>
							name1%>
						</h2>
						<h3>
							contact1%>
						</h3>
						<p>
							relation1%>
						</p>
					</div>
				</div> -->


				<div class="yui-gf">
					<div class="yui-u first">
						<h2>Skills</h2>
					</div>
					<div class="yui-u" id="skillsContainer">
						<script>
							const arr = '<%=skills%>'
						 function makeList(){ 
							const container = document.getElementById('skillsContainer')
							const items = arr.split(',')
							const ul = document.createElement('ul');
							items.forEach(function(item){
								const li = document.createElement('li');
								li.textContent = item.trim();
								li.setAttribute('class', 'skills-li')
								ul.appendChild(li)
							});
							container.appendChild(ul)
						} 
						makeList()
						</script>
					</div>
				</div>


			<!--	<div class="yui-gf last">
					<div class="yui-u">
						<h2>
							name2%>
						</h2>
						<h3>
							contact2%>
						</h3>
						<p>
							relation2%>
						</p>
					</div>
				</div>


			</div>
		</div> -->

	</div>
	<!-- // bd -->

	<!--// footer -->

	</div><!-- // inner -->

	<button class="removeInPdf btn btn-danger"> <a id="downloadLink" href="http://localhost:3000/downloadPdf">Download your resume in pdf</a></button>
	<!-- <button class="removeInPdf"> <a href="http://localhost:3000/emailPdf">Send by email</a></button> -->
	</div>

	


</body>

</html>